{
  "name": "pomodo_electron",
  "version": "1.0.0",
  "description": "Pomodoro timer written in electron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Pomodron\"",    
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/umutkoksaldi/pomodron.git"
  },
  "keywords": [
    "pomodoro"
  ],
  "author": "umutkoksaldi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/umutkoksaldi/pomodron/issues"
  },
  "homepage": "https://github.com/umutkoksaldi/pomodron#readme",
  "dependencies": {
    "electron": "^1.7.9"
  },
  "devDependencies": {
    "electron-packager": "^9.1.0"
  }
}
